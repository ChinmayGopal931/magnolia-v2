{
  "name": "magnolia-v2",
  "version": "1.0.0",
  "description": "Delta neutral position tracker for perpetual DEXes",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:setup": "tsx scripts/setup-database.ts",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx src/scripts/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:types": "./scripts/generate-db-types.sh",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "trading",
    "defi",
    "hyperliquid",
    "drift"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@drift-labs/sdk": "^2.128.0-beta.7",
    "@nktkas/hyperliquid": "^0.23.1",
    "@solana/web3.js": "^1.98.2",
    "@types/pg": "^8.15.4",
    "axios": "^1.10.0",
    "bs58": "^6.0.0",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.3",
    "ethers": "^6.10.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.3",
    "tweetnacl": "^1.0.3",
    "viem": "^2.31.7",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "drizzle-kit": "^0.20.9",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
